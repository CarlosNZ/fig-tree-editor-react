{
  "name": "fig-tree-demo",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://carlosnz.github.io/fig-tree-evaluator",
  "type": "module",
  "scripts": {
    "start": "rimraf ./src/fig-tree-evaluator && rimraf ./src/expression-builder && mkdir ./src/fig-tree-evaluator && mkdir ./src/fig-tree-evaluator/src && mkdir ./src/expression-builder && mkdir ./src/expression-builder/src && concurrently --kill-others-on-fail \"yarn serve\" \"yarn dev\" \"nodemon watch.js\"",
    "serve": "node ./src/express/server",
    "dev": "vite --host",
    "build": "rimraf dist && tsc && vite build && cp -r img dist/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist"
  },
  "nodemonConfig": {
    "watch": [
      "../src",
      "../expression-builder/src"
    ],
    "ignore": [
      "../expression-builder/src/fig-tree-evaluator"
    ],
    "ext": "js, json, ts, tsx, txt",
    "delay": 200
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fig-tree-evaluator": "^2.19.1",
    "framer-motion": "^10.15.1",
    "json-edit-react": "^1.22.1",
    "json5": "^2.2.3",
    "markdown-to-jsx": "^7.4.7",
    "pg": "^8.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "use-undo": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.9",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "gh-pages": "^6.1.1",
    "nodemon": "^3.0.1",
    "react-select": "^5.8.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
